{
  "name": "documentation",
  "scripts": {
    "dev": "nuxt dev --extends docus",
    "build": "nuxt build --prerender --extends docus",
    "lint": "oxlint -c config/oxlintrc.json .",
    "format": "oxfmt .",
    "format:check": "oxfmt --check .",
    "type:check": "tsc --noEmit",
    "prepare": "prek install",
    "update": "up --latest",
    "clean": "rm -rf node_modules .nuxt .output .cache .husky .data .content dist && pnpm store prune",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --project unit",
    "test:nuxt": "vitest --project nuxt"
  },
  "dependencies": {
    "@googlemaps/markerclusterer": "^2.6.2",
    "@nuxt/content": "3.10.0",
    "@nuxt/image": "2.0.0",
    "@nuxt/scripts": "0.13.2",
    "@nuxt/test-utils": "3.23.0",
    "@nuxt/ui": "4.3.0",
    "@unhead/vue": "^2.1.2",
    "better-sqlite3": "^12.6.0",
    "docus": "latest",
    "nuxt": "^4.2.2",
    "oxlint": "latest",
    "typescript": "^5.9.3",
    "zod": "^4.3.5",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@j178/prek": "latest",
    "@vitest/coverage-v8": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.1.0",
    "oxfmt": "latest",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.28.0"
}
