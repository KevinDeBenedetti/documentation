{
  "name": "documentation",
  "scripts": {
    "dev": "nuxt dev --extends docus",
    "build": "nuxt build --prerender --extends docus",
    "lint": "oxlint -c oxlintrc.json .",
    "format": "oxfmt .",
    "format:check": "oxfmt --check .",
    "type:check": "tsc --noEmit",
    "prepare": "prek install",
    "update": "up --latest",
    "clean": "rm -rf node_modules .nuxt .output .cache .husky .data .content dist coverage && pnpm store prune",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --project unit",
    "test:nuxt": "vitest --project nuxt"
  },
  "dependencies": {
    "@googlemaps/markerclusterer": "^2.6.2",
    "@nuxt/content": "^3.11.2",
    "@nuxt/image": "2.0.0",
    "@nuxt/scripts": "0.13.2",
    "@nuxt/test-utils": "4.0.0",
    "@nuxt/ui": "4.4.0",
    "@unhead/vue": "^2.1.4",
    "better-sqlite3": "^12.6.2",
    "docus": "5.6.1",
    "nuxt": "^4.3.1",
    "oxlint": "1.49.0",
    "typescript": "^5.9.3",
    "unist-util-visit": "^5.1.0",
    "zod": "^4.3.6",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@j178/prek": "0.3.3",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.6.3",
    "memfs": "^4.56.10",
    "oxfmt": "0.34.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.28.0"
}
